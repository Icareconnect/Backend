{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "amrnn/laravel-cursor-paginator": "^1.1",
        "authorizenet/authorizenet": "^2.0",
        "authy/php": "3.0.5",
        "barryvdh/laravel-dompdf": "^0.8.7",
        "cartalyst/stripe-laravel": "^11.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "gecche/laravel-multidomain": "2.*",
        "intervention/image": "^2.5",
        "ixudra/curl": "^6.19",
        "jeroennoten/laravel-adminlte": "^3.1",
        "laravel-notification-channels/webpush": "^5.0",
        "laravel/framework": "^6.2",
        "laravel/passport": "^8.4",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "3.3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "malhal/laravel-geographical": "^1.0",
        "paypal/rest-api-sdk-php": "^1.14",
        "pragmarx/countries": "^0.7.0",
        "predis/predis": "^1.1",
        "pulkitjalan/google-apiclient": "^4.1",
        "razorpay/razorpay": "2.*",
        "stevebauman/location": "^5.2",
        "stripe/stripe-php": "^7.28",
        "tanmuhittin/laravel-google-translate": "^2.0",
        "twilio/sdk": "6.4.0",
        "yabacon/paystack-php": "^2.2",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/seeds/state-cities",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
